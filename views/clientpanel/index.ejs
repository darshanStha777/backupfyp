<!DOCTYPE html>
<html lang="en">

<head>

    <%- include("../include/head.ejs") %>
        <style>
            main {
                padding-left: 0px !important;
            }
        </style>


</head>

<body>

    <header style="overflow-x: scroll;">



        <!-- Navbar -->
        <!--Main Navigation-->
        <header>
            <!-- Navbar -->
            <nav id="main-navbar" class=" row navbar navbar-expand-lg navbar-light bg-white fixed-top">
                <!-- Container wrapper -->
                <div class="container-fluid ">
                    <!-- Brand -->
                    <a class="navbar-brand" href="#">
                        <img src="/vendor/image/team-management--removebg-preview.png" height="50" alt="" loading="lazy" />
                    </a>

                    <!-- Right links -->
                    <ul class="navbar-nav ms-auto d-flex flex-row">


                        <li class="nav-item dropdown">
                            <div class="dropdown pe-5 mt-1">

                                <a class="font-uppercase" id="dropdownMenuButton2" role="" data-bs-toggle="dropdown" aria-expanded="false">
                                    <span class="text-uppercase"> <%= clientdetails.clientName %> <i class="fas fa-angle-down ms-2"></i> </span>

                                </a>

                                <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="dropdownMenuButton2">
                                    <li><a class="dropdown-item" href="/client/profile">My Profile</a></li>
                                    <li><a class="dropdown-item" href="/client/logout">Logout</a></li>
                                </ul>
                            </div>
                        </li>
                    </ul>
                </div>


            </nav>
            <!-- Navbar -->


        </header>


    </header>
    <!--Main Navigation-->

    <!--Main layout-->
    <main style="margin-top: 79px " class=" ">
        <div class="container-fluid mt-1 navbar-light bg-white">
            <div class="row d-flex justify-content-around">
                <div class="col-md-4 d-flex  justify-content-center ">
                    <div>
                        <a href="/client/" class="nav-item nav-link text-primary "> <i class="fas fa-tasks"></i> Project </a>
                    </div>
                    <div>
                        <a href="/client/paymentpage" class="nav-item nav-link text-dark "> <i class="fas fa-credit-card"></i> Payment</a>
                    </div>
                    <div>
                        <a href="/client/message" class="nav-item nav-link text-dark "> <i class="fas fa-envelope"></i> Message</a>
                    </div>

                </div>
            </div>
        </div>
        <!--Section: Minimal statistics cards-->
        <!--Section: Minimal statistics cards-->

        <!--Section: Minimal statistics cards-->

        <div class="container-fluid pt-4  ">
            <!-- Section: Main chart -->
            <section class="team  py-1">
                <div class="container">
                    <div class="header my-1 text-center">
                        <h1>Project Details </h1>

                    </div>
                    <div class="row">
                        <table id="" class="display table table-hover" style="width:100%">
                            <thead>
                                <tr class="bg-dark text-light">
                                    <th>S.N</th>
                                    <th>Project Name</th>
                                    <th>Start Date</th>
                                    <th>End Date</th>
                                    <th>Project Progress</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for (var i = 0; i < projectdetails.length; i++) { %>
                                    <tr>
                                        <th>
                                            <%= i + 1  %>
                                        </th>
                                        <th>

                                            <%= projectdetails[i].projectName  %>
                                        </th>
                                        <th>
                                            <input type="text" id="startdate" class="border-0" readonly value=" <%= projectdetails[i].projectstartdate  %>" placeholder>

                                        </th>

                                        <th>
                                            <input type="text" id="enddate" class="border-0" readonly value=" <%= projectdetails[i].projectenddate  %>" placeholder>


                                        </th>
                                        <th>
                                            <div>
                                                <div class=" border border-primary p-1">
                                                    <div class="progress " style="height:15px">
                                                        <div id="progressbar" class="progress-bar bg-success p-2 text-dark" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:70%">
                                                            70% Complete
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </th>
                                        <th>
                                            <div>
                                                <a href="/src/teamsubmitproject/<%= projectdetails[i].submittoclient  %>" class="btn btn-primary h-25" download>Download </a>
                                            </div>
                                        </th>

                    </div>

                    </th>
                    </tr>
                    <% } %>

                        </tbody>

                        </table>
                </div>
        </div>
        </section>
        <!-- Section: Main chart -->
        </div>

    </main>
    <!-- MDB -->
    <%- include("../include/footer.ejs") %>
        <script>
            var startdate = document.getElementById("startdate").value;
            var enddate = document.getElementById("enddate").value;

            var date = new Date(startdate);
            var date2 = new Date(enddate)

            console.log(date);
            console.log(date2);


            $(document).ready(function() {
                var countDownDate = new Date(date2).getTime();
                var startDate = new Date(date).getTime();
                // Update the count down every 1 second
                // Get todays date and time
                var now = new Date().getTime();
                console.log(now)

                // Find the distance between now and the count down date
                var distanceWhole = countDownDate - startDate;
                var distanceLeft = countDownDate - now;

                // Time calculations for minutes and percentage progressed
                var minutesLeft = Math.floor(distanceLeft / (1000 * 60));
                var minutesTotal = Math.floor(distanceWhole / (1000 * 60));
                var progress = Math.floor(((minutesTotal - minutesLeft) / minutesTotal) * 100);
                $('#progressbar').attr('aria-valuenow', progress).css('width', progress + "%").html(progress + "% Complete");
            });
        </script>



</body>

</html>